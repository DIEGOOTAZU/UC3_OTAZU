<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E2CEE2F1"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView12"
        android:layout_width="278dp"
        android:layout_height="27dp"
        android:layout_marginTop="16dp"
        android:text="¿Por qué tengo que dar esta información?"
        android:textColor="#242E61"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtfechanac" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="155dp"
        android:layout_height="40dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="45dp"
        android:background="#003F51B5"
        android:text="Registrate"
        android:textColor="#242E61"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="201dp"
        android:layout_height="22dp"
        android:layout_marginStart="41dp"
        android:layout_marginTop="10dp"
        android:text="Es gratis ( y lo seguirá siendo)"
        android:textColor="#242E61"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <EditText
        android:id="@+id/txtApellido"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:backgroundTint="#1631D1"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="#343539"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/txtNombre" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="76dp"
        android:layout_marginTop="32dp"
        android:text="Nombre:"
        android:textColor="#242E61"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="24dp"
        android:text="Apellido:"
        android:textColor="#242E61"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <EditText
        android:id="@+id/txtNombre"
        android:layout_width="209dp"
        android:layout_height="40dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="128dp"
        android:backgroundTint="#1631D1"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="#26272B"
        android:textColorHint="#EAE5E5"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="119dp"
        android:layout_height="40dp"
        android:layout_marginTop="16dp"
        android:text="Tu dirección de correo electronico:"
        android:textColor="#242E61"
        app:layout_constraintEnd_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <EditText
        android:id="@+id/txtCorreo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:backgroundTint="#1631D1"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:shadowColor="#E8E1E1"
        android:textColor="#26272B"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/txtApellido" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:text="Contraseña nueva:"
        android:textColor="#242E61"
        app:layout_constraintEnd_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <EditText
        android:id="@+id/txtpass"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:backgroundTint="#1631D1"
        android:ems="10"
        android:inputType="textPassword"
        android:textColor="#26272B"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/txtCorreo" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="Sexo:"
        android:textColor="#242E61"
        app:layout_constraintEnd_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <RadioGroup
        android:id="@+id/radioGroup"
        android:layout_width="240dp"
        android:layout_height="47dp"
        android:layout_marginStart="1dp"
        android:orientation="horizontal"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/txtpass">

        <RadioButton
            android:id="@+id/radioButton"
            android:layout_width="111dp"
            android:layout_height="match_parent"
            android:layout_marginStart="15dp"
            android:backgroundTint="#1631D1"
            android:buttonTint="#1631D1"
            android:text="Mujer"
            android:textColor="#26272B" />

        <RadioButton
            android:id="@+id/radioButton2"
            android:layout_width="111dp"
            android:layout_height="44dp"
            android:backgroundTint="#1631D1"
            android:buttonTint="#1631D1"
            android:text="Hombre"
            android:textColor="#26272B" />
    </RadioGroup>

    <TextView
        android:id="@+id/textView10"
        android:layout_width="80dp"
        android:layout_height="38dp"
        android:layout_marginTop="16dp"
        android:text="Fecha de nacimiento:"
        android:textColor="#242E61"
        app:layout_constraintEnd_toEndOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <EditText
        android:id="@+id/txtfechanac"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:backgroundTint="#1631D1"
        android:ems="10"
        android:inputType="date"
        android:textColor="#26272B"
        android:textColorHint="#E2E3EA"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/radioGroup" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="278dp"
        android:layout_height="27dp"
        android:layout_marginTop="16dp"
        android:text="¿Por qué tengo que dar esta información?"
        android:textColor="#242E61"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtfechanac" />

    <Button
        android:id="@+id/buttonregistrar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:backgroundTint="#5ECA62"
        android:drawableLeft="@android:drawable/ic_menu_save"
        android:text="Regístrate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <EditText
        android:id="@+id/editTextTextPersonName3"
        android:layout_width="425dp"
        android:layout_height="31dp"
        android:backgroundTint="#122BB3"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintTop_toBottomOf="@+id/buttonregistrar"
        tools:layout_editor_absoluteX="-7dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="368dp"
        android:layout_height="42dp"
        android:text="Crear una pagina para un famosos grupo de musica o empresa"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName3" />

</androidx.constraintlayout.widget.ConstraintLayout>